package chap04;

public class ForExam03 
{

	public static void main(String[] args) 
	{
		System.out.println("1 to 100까지의 수");
		System.out.println();

		int sum1=0;
		int sum2=0;
		int sum3=0;	
		
		for(int i=1; i<=100; i++)
		{
			sum1 = sum1+i;
		}
		
		System.out.println("모든 수의 합: "+sum1);
	
	
	
	
	}	
}

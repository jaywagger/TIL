package chap04;
//for문 연습 - 기본 for 연습(for문의 문법)
public class ForTest01 
{
	/**
	 * @param args
	 */
	public static void main(String[] args) 
	{
		for(int i=1; i<5;i++)
			{
			System.out.println("자바프로그래밍");
			}
		
		System.out.println("***********************************");
		for(int i=11; i<25;i++)
		{
		System.out.println("자바프로그래밍");
		}
		
		System.out.println("***********************************");
		for(int i=5; i<1;i--) //조건에 따라 --도 사용할 수 있다.
		{
		System.out.println("자바프로그래밍");
		}
	
	}
}

package Workbook;

public class Workbook02 {

	public static void main(String[] args) 
	{
		String s1 = "1";
		String s2 = "2";
		boolean bnx = false;
		char c1 = 'A';
		char c2 = 'B';
		char c3 = '1';
		char c4 = '2';
		int inx = 2;
		// "" 와 '' 차이를 유념한다. 
		
		System.out.println("ascii 값 'A' -> ");
		System.out.println();
		
		System.out.println("ascii 값 'B' -> ");
		System.out.println();
		
		System.out.println("ascii 값 '1' -> ");
		System.out.println();
		
		System.out.println("ascii 값 '2' -> ");
		System.out.println();


	}

}

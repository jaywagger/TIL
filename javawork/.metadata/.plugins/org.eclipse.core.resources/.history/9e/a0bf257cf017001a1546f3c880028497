package chap04;
//기초 while문 연습
//자바 프로그래밍 5번 출력
public class WhileTest01 {

	public static void main(String[] args) 
	{
		int i=1;
		while(i<=5)
		{
			System.out.println(i+". 자바프로그래밍");
			i++; 
		}
		System.out.println("---------------------------------");
		while(true)
		{
			
		}
		
		
		
	}

}

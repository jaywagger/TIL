package chap04;

public class DoWhileTest {

	public static void main(String[] args) 
	{
		int num=10;
		while(num<10)
		{
			//while문은 조건을 만조하지 앟으면 브럭만의 코드가 실행되지 않는다
			System.out.println("while문- 실행명령문:"+num);
			num++;
		}
		
		num=10;
		do
		{
			System.out.println("Do_While문 - 실행명령문: "+num);
			num++;
		}
		while(num<10);

	}

}
